{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		},
		"allowImportingTsExtensions": true
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"setup-tests.js",
		"server/routes/users.js",
		"server/routes/pokemons.js",
		"server/server.js"
	]
}
