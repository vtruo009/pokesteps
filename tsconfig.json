{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		},
		"allowImportingTsExtensions": true
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"setup-tests.js",
		"api/routes/users.ts",
		"api/routes/pokemons.ts",
		"api/index.ts"
	]
}
